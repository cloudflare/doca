{
  "id": "/profile",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Profile",
  "description": "A Profile within safewire",
  "type": "object",
  "definitions": {
    "transaction-id": {
      "type": "string",
      "description": "Transaction ID sent by SoftPro for each party upon order submission",
      "example": "0000-000-000000-0000001"
    },
    "first_name": {
      "type": "string",
      "description": "First Name",
      "example": "John"
    },
    "last_name": {
      "type": "string",
      "description": "Last name",
      "example": "Stark"
    },
    "party-type": {
      "type": "string",
      "description": "What type of a party is it (Byer/Seller)",
      "example": "Seller"
    },
    "legal-entity-name": {
      "type": "string",
      "description": "Name of the legal entity",
      "example": "Burger King"
    },
    "legal-entity-type": {
      "type": "string",
      "description": "Type of the legal entity",
      "example": "llc"
    },
    "mobile-phone": {
      "type": "string",
      "description": "Mobile phone",
      "example": "+1614111111"
    },
    "email": {
      "type": "string",
      "description": "Email From Profile",
      "example": "sample@sample.com"
    },
  },
  "required": [
    "transaction-id",
    "first_name",
    "last_name",
    "party-type",
    "mobile-phone",
    "email",
  ],
  "properties": {
    "transaction-id": {"$ref": "#/definitions/transaction-id"},
    "first_name": {"$ref": "#/definitions/first_name"},
    "last_name": {"$ref": "#/definitions/last_name"},
    "party-type": {"$ref": "#/definitions/last_name"},
    "legal-entity-name": {"$ref": "#/definitions/legal-entity-name"},
    "legal-entity-type": {"$ref": "#/definitions/legal-entity-type"},
    "mobile-phone": {"$ref": "#/definitions/mobile-phone"},
    "email": {"$ref": "#/definitions/email"},
  }
}
